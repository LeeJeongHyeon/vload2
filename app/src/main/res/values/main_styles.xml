<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SignupHeaderMenuLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="SignupHeaderMenuImageButton">
        <item name="android:layout_width">15.3dp</item>
        <item name="android:layout_height">15.3dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:padding">12dp</item>
    </style>
    <style name="SignupHeaderMenuLayoutTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/bmjua</item>
        <item name="android:gravity">center</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:lineSpacingExtra">33.3sp</item>
        <item name="android:textSize">18.3sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>


</resources>